FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    U
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        relTol          0.01;
    }
}

stressedFoam
{
    nCorrectors     1;
    U               1e-06;

    componentReference
    (
        {
            patch down;
            face  0;
            direction z;
            value 0;
        }
    );
}

