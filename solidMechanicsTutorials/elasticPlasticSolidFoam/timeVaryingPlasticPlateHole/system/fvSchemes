FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default     Euler;
}

gradSchemes
{
    default         none;
    grad(DU)         extendedLeastSquares 0;
    snGradCorr(DU)   extendedLeastSquares 0;
}

divSchemes
{
    default         none;
    div(sigma)      Gauss linear;
}

laplacianSchemes
{
    default         none;
    laplacian(DDU,DU) Gauss linear corrected;
}

snGradSchemes
{
    snGrad(DU)       corrected;
}

interpolationSchemes
{
    default                  linear;
}

