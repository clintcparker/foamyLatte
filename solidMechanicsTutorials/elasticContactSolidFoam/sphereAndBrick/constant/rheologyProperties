FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          rheologyProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

rheology
 {
     type            linearElastic;
     rho             rho [1 -3 0 0 0 0 0] 7854;
     E               E [1 -1 -2 0 0 0 0] 200e9;
     nu              nu [0 0 0 0 0 0 0] 0.3;
 }


