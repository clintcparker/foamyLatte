FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      DU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
  surface
    {
      type            fixedRotation;
      rotationAngle   1;
      rotationAxis    ( 0 0 1 );
      rotationOrigin  ( 0 0 0 );
      value uniform   ( 0 0 0 );
    }
}

