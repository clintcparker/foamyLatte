FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    	default       steadyState;
}

gradSchemes
{
	default		extendedLeastSquares 0;
}

divSchemes
{
	default         none;
 	div(sigma)      Gauss linear;
}

laplacianSchemes
{
	default         none;
	laplacian(DDU,DU) Gauss linear corrected;
}


snGradSchemes
{
	snGrad(DU)     corrected;
}

interpolationSchemes
{
	default			linear;
}

