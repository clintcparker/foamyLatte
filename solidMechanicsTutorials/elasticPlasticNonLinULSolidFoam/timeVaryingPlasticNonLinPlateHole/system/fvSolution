FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    DU
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-7;
        relTol          0.1;
    }
}

stressedFoam
{
    nCorrectors     10000;
    DU              1e-6;

    divDSigmaExp             surface;
    divDSigmaLargeStrainExp  surface;
    moveMeshMethod           leastSquares;
    solidInterface           no;
}

relaxationFactors
{
  DU         0.95;
}
